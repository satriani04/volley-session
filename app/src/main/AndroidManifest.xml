<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.saputro.volleysession" >

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />

    <application
        android:name=".com.example.saputro.library.MyApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".LoginActivity"
            android:label="@string/title_activity_login" >
        </activity>
        <activity
            android:name=".ListBlogActivity"
            android:label="@string/title_activity_list_blog" >
        </activity>
        <activity
            android:name=".DetailActivity"
            android:label="@string/title_activity_detail" >
        </activity>
        <activity
            android:name=".com.example.saputro.library.AddBlogActivity"
            android:label="@string/title_activity_add_blog" >
        </activity>
        <activity
            android:name=".AddBlogActivity"
            android:label="@string/title_activity_add_blog" >
        </activity>
        <activity
            android:name=".UpdateActivity"
            android:label="@string/title_activity_update" >
        </activity>
        <activity
            android:name=".VolleyListActivity"
            android:label="@string/title_activity_volley_list" >
        </activity>
        <activity
            android:name=".FormUpdateActivity"
            android:label="@string/title_activity_form_update" >
        </activity>
    </application>

</manifest>
